<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.rac.model.PatronForms" table="PatronForms" lazy="false">

		<id name="patronFormId" column="patronFormId" type="java.lang.Long">
			<generator class="native">
            </generator>
		</id>
		<version name="version" type="java.lang.Long" column="version"/>

		<property name="auditInfo" type="org.archiviststoolkit.hibernate.AuditInfoType">
			<column name="lastUpdated"/>
			<column name="created"/>
			<column name="lastUpdatedBy"/>
			<column name="createdBy"/>
		</property>

		<property name="formType"
				  column="formType"
				  type="string"/>

		<property name="notes"
				  column="notes"
				  type="text"/>

        <property name="dateCompleted"
                  column="dateCompleted"
                  type="date"/>

        <property name="completed"
                  column="completed"
                  type="boolean"/>

        <many-to-one name="patron"
                     class="org.rac.model.Patrons"
                     column="patronId"/>

	</class>
</hibernate-mapping>
